<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="4dp"
                android:color="#9ADAFE"
                android:dashWidth="10dp"
                android:dashGap="4dp" />

            <solid android:color="#FFF" />

            <corners
                android:radius="18dp"/>
        </shape>
    </item>
</layer-list>